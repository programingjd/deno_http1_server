{
  "$schema": "http://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/programingjd/deno_http1_server/blob/main/mimetypes.schema.json",
  "title": "MimeTypes",
  "description": "File type configs for the different accepted mime types",
  "type": "object",
  "patternProperties": {
    "^[a-z]+/[a-z0-9.+-]+$": {
      "oneOf": [
        {
          "type": "string"
        },
        {
          "$ref": "https://github.com/programingjd/deno_http1_server/blob/main/mimetype_config.schema.json"
        }
      ]
    }
  },
  "additionalProperties": false
}
